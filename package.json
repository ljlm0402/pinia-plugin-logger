{
  "name": "pinia-plugin-logger",
  "version": "1.1.0",
  "description": "Logger for Pinia",
  "author": "AGUMON <ljlm0402@gmail.com>",
  "license": "MIT",
  "keywords": [
    "vue",
    "pinia",
    "state",
    "store",
    "plugin",
    "logger"
  ],
  "source": "src/index.ts",
  "main": "dist/pinia-plugin-logger.js",
  "module": "dist/pinia-plugin-logger.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/pinia-plugin-logger.es.js",
      "require": "./dist/pinia-plugin-logger.js",
      "types": "./dist/index.d.ts"
    }
  },
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/ljlm0402/pinia-plugin-logger.git"
  },
  "bugs": {
    "url": "https://github.com/ljlm0402/pinia-plugin-logger/issues"
  },
  "homepage": "https://github.com/ljlm0402/pinia-plugin-logger#readme",
  "scripts": {
    "build": "rimraf dist && microbundle --external all --name piniaPluginLogger",
    "prepare": "npm run build",
    "clean": "rimraf ./dist"
  },
  "dependencies": {},
  "devDependencies": {
    "bundlesize": "^0.18.2",
    "husky": "^9.1.7",
    "microbundle": "^0.15.1",
    "pinia": "^3.0.1",
    "prettier": "^3.5.2",
    "pretty-quick": "^4.0.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.7.3"
  },
  "peerDependencies": {
    "pinia": "^2.0.0 || ^3.0.0",
    "vue": "^3.2.0"
  },
  "bundlesize": [
    {
      "path": "./dist/*.js",
      "threshold": "5KB"
    }
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
