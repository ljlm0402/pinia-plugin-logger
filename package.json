{
  "name": "pinia-plugin-logger",
  "version": "0.0.1",
  "description": "Logger for Pinia",
  "author": "AGUMON <ljlm0402@gmail.com>",
  "license": "MIT",
  "keywords": [
    "vue",
    "pinia",
    "state",
    "store",
    "plugin",
    "logger"
  ],
  "source": "src/index.ts",
  "main": "dist/pinia-plugin-logger.js",
  "module": "dist/pinia-plugin-logger.es.js",
  "unpkg": "dist/pinia-plugin-logger.umd.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rimraf dist && microbundle --external all --name piniaPluginLogger",
    "prepare": "npm run build"
  },
  "dependencies": {},
  "devDependencies": {
    "bundlesize": "^0.18.1",
    "dayjs": "^1.11.7",
    "husky": "^8.0.3",
    "microbundle": "^0.15.1",
    "pinia": "^2.0.34",
    "prettier": "^2.8.7",
    "pretty-quick": "^3.1.3",
    "rimraf": "^5.0.0",
    "typescript": "^5.0.4"
  },
  "peerDependencies": {
    "pinia": "^2.0.0",
    "vue": "^3.0.0"
  },
  "bundlesize": [
    {
      "path": "./dist/*.js",
      "threshold": "800b"
    }
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run build && pretty-quick --staged"
    }
  }
}
